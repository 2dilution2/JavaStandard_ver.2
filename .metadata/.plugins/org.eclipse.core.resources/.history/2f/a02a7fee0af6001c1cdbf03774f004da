
public class Ex2_1 {

	public static void main(String[] args) {
		// cntrl + shift + alt + down	행단위 복사
		// alt + shift + A				멀티 컬럼 모드 (토글)
		// print() - 출력후에 줄 안바꿈
		// pritnln() - 출력후에 줄바꿈
		System.out.println(5+3);
		System.out.println(5-3);
		System.out.println(5*3);
		System.out.println(5/3);
	}

}
